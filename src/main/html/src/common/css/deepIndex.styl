#mainBanner
  width 100%
  height 376px
  background url("../../../static/images/banner.png")
  background-size 100% 376px
  margin-top 44px
  position relative
  #dataChange
    //margin-top 20px
    width 370px
    height 140px
    display inline-block
    //border 1px solid red
    margin-top 40px
    img
      vertical-align bottom
  #dataAdvert
    display inline-block
    //display inline-block
    //border 1px solid red
    vertical-align top
    margin-top 40px
    h1
      font-size 2.4em
      color #339FFB
      line-height 45px
      font-family '宋体'
      font-weight bold

  #datadesc
    text-align center
    width 100%
    color #ffffff
    height 63px
    line-height 63px
    position absolute
    left 0px
    bottom 0px
    span
      margin-right 10px
    &:last-child
      margin-right 0px;
#industry
  padding 15px
  #indMain
    padding 10px
    border 1px solid #ebeef5
    .title
      font-size 1.2em
      color #3399FF
      font-weight bold
      line-height 35px
      white-space pre-wrap
      word-break break-all
      overflow hidden
      text-overflow ellipsis
      cursor pointer
    #indIconUl
      width 100%
      min-width 600px
      //border 1px solid red
      li
        text-align center
        width 10%
        height 120px
        float left
        box-sizing border-box
        margin-bottom 10px
        border 1px solid #F0F0F0
        #imgpath
          width 80px
          height 80px
          border-radius 50%
          border 1px solid #F0F0F0
          margin 5px auto
          img
            width 50px
            height 50px
            margin-top 15px
      li.active
        box-shadow 2px 2px 2px #6e6e6e

    .ActiveUl
      max-height 260px
      overflow hidden
    .ActiveNo
      height auto
      overflow auto
#input
  padding 15px
#dataSet
  padding 15px

  #dataSetLeft
    width 130px
    height 130px
    margin 10px 10px
    //border 1px solid red
    //img
    //  margin 25px
  #dataSetRight
    flex 1
    display flex
    border-bottom 1px solid #ebeef5
    padding-right 15px
    #dsRleft
      flex 1
      float left
      padding 0px
      .title
        font-size 1.1em
        line-height 25px
        margin-top 10px
      .titleName
        color #4465B0
        overflow hidden
        text-overflow ellipsis
        white-space pre
        word-wrap normal !important
        word-break normal !important
      .desc,.descTime
        font-size 1em
        word-wrap break-word
        line-height 25px
        letter-spacing 1px
      .descPublic
        font-size 1.1em
        line-height 35px
        overflow hidden
        text-overflow ellipsis
        white-space pre
        word-wrap normal !important
        word-break normal !important
    #dsRright
      float right
      width 300px
      display flex
      .dsrPrice
        margin-top 50px
        text-align right
        width 140px
        .price
          color orange
          font-size 1.1em
      #btn
        flex 1
        //border 1px solid red
        .el-button
          padding 10px 45px
          float right
          margin 5px 0px
          &:last-child
            margin-bottom 0px
#descData
  padding 0px 40px
  display flex
  margin-bottom 20px
  margin-top -20px
  //border 1px solid red
  #descDataLeft
    width 100px
    height 100px
  #descDataContent
    flex 1
    text-align center
    line-height 80px
  #descDataRight
    width 200px
    height 72px
    margin-top 10px
    //border 1px solid red
    //border 1px solid red
    .el-button
      float right
      padding 10px 45px
      &:first-child
        margin-top 0px
        margin-bottom 4px
      &:last-child
        margin-bottom 0px
#descTable
  border-collapse collapse
  width 100%
  th,td
    border 1px solid #ebeef5
    white-space pre-wrap
  th
    width 40%
    text-align center
